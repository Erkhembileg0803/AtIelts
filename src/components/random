   useEffect(() => {
        const container = containerRef.current;
        const handleWheel = (e) => {
            if(e.deltaY !== 0){
                container.scrollLeft += e.deltaY;
                e.preventDefault();
            }
        }
        container.addEventListener('wheel',handleWheel);
        return () => {
            container.removeEventListener('wheel', handleWheel);
        }
    }, [])